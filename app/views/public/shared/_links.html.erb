<%- if controller_name != 'sessions' %>
  <%= link_to "登録済みの方はこちらからログイン", new_session_path(resource_name), class: "sign-up-link" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "新規登録がまだの方はこちらから新規登録をお願いします", new_registration_path(resource_name), class: "sign-in-link" %><br />
<% end %>
