<%= form_with model: [@group, @notice] do |f| %>
  <div class="field">
    <%= f.label :title, "1.件名を入力" %>
    <%= f.text_field :title, autofocus: true %>
  </div>
  <div class="field">
    <%= f.label :body, "2.本文を入力" %>
    <%= f.text_area :body %>
  </div>
  <div class="field">
    <%= f.submit "送信" %>
  </div>
<% end %>